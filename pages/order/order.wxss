.header_{width: 100%;height: 6vh;box-shadow:0 0 10px #B4BCCC;position: fixed;top: 0;background-color: white;z-index: 999999;}
.span_33{display: inline-block;float: left;width:30%;text-align: center;font-family: '宋体';font-size: 30rpx;line-height: 6vh;}
.contain{height: 94vh;margin-top: 6vh;}
.icons{line-height: 2vh;margin-left: 1vw;display: inline-block;height: 0.5vh}
.bor{position: fixed;width: 100vw;height: 1vh;background-color: black;opacity: 0.65;top: 6vh;left: 0;right: 0;margin: auto;transition: 0.5s;}
.bord{width: 100vw;height: 1px;border-color: #B4BCCC;}
.orderid{margin-top: 1vh;}
.scroll_view>view{text-indent: 1em;font-family: '宋体';font-size: 30rpx;color: #606266;line-height: 4vh;}
.line{width: 96vw;height: 1px;border-bottom: 1px solid #B4BCCC;margin: auto;color: #dddddd}
.auth{display: inline-block;width: 50vw;}